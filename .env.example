# === App ===
APP_ENV=local

# === Postgres (docker-compose default) ===
POSTGRES_USER=expense
POSTGRES_PASSWORD=expense
POSTGRES_DB=expense_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# === Backend DB connection (local) ===
DATABASE_URL=postgresql+psycopg://expense:expense@localhost:5432/expense_db

# SQLite fallback example (no Docker):
# DATABASE_URL=sqlite:///./expense_manager.db

# SQLAlchemy query logging (true/false)
SQL_ECHO=false
